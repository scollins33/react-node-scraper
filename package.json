{
  "name": "eminence",
  "version": "1.0.0",
  "description": "React stack app to summarize data from various sources",
  "main": "server/dist/server.js",
  "scripts": {
    "dev": "concurrently \"npx webpack --config ./client/webpack.config.js --watch\" \"tsc -p ./server/tsconfig.json --watch\" \"nodemon --watch server/dist\"",
    "watchClient": "npx webpack --config ./client/webpack.config.js --watch",
    "watchServer": "tsc -p ./server/tsconfig.json --watch",
    "buildServer": "tsc -p ./server/tsconfig.json",
    "nodemon": "nodemon --watch server/dist"
  },
  "proxy": "http://localhost:8080",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scollins33/eminence.git"
  },
  "author": "Sean Collins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/scollins33/eminence/issues"
  },
  "homepage": "https://github.com/scollins33/eminence#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "fetch-cookie": "^0.7.3",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.45",
    "concurrently": "^5.1.0",
    "create-react-app": "^3.4.0",
    "css-loader": "^3.4.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
